{
    "name": "rivals-rankings-smashgg",
    "url": "https://github.com/kennethwang14/rivals-rankings.git",
    "version": "1.0.0",
    "description": "Rivals Rankings using SmashGG",
    "scripts": {
        "build": "next build",
        "dev": "next",
        "lint": "eslint \"{components,lib,pages,scripts}/**/*.js{,.flow}\"",
        "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
        "flow": "flow",
        "graphql-get": "graphql get-schema",
        "graphql-gen": "apollo-codegen introspect-schema schema.graphql --output schema.json && apollo-codegen generate components/_/*/*.js --schema schema.json --target flow --output components/_/graphql-types.js.flow --add-typename true --merge-in-fields-from-fragment-spreads false",
        "import-data": "./node_modules/babel-cli/bin/babel-node.js ./scripts/import-data/index.js",
        "start": "next start"
    },
    "author": "Kenneth Wang",
    "license": "ISC",
    "devDependencies": {
        "apollo-codegen": "^0.18.3",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.0.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "bluebird": "^3.5.1",
        "eslint": "^4.13.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-flowtype": "^2.41.1",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "^2.5.0",
        "eslint-plugin-react": "^7.5.1",
        "flow-bin": "^0.64.0",
        "glicko2": "^0.8.4",
        "got": "^8.0.1",
        "graphql-cli": "^2.12.4",
        "graphql-request": "^1.4.0",
        "husky": "^0.14.3",
        "lint-staged": "^6.0.1",
        "lodash": "^4.17.4",
        "prettier": "^1.9.2",
        "redux": "^3.7.2"
    },
    "dependencies": {
        "apollo-client-preset": "^1.0.5",
        "apollo-link-http": "^1.3.1",
        "glamor": "^2.20.40",
        "glamorous": "^4.11.0",
        "graphql": "^0.12.3",
        "graphql-anywhere": "^4.1.0",
        "graphql-tag": "^2.6.0",
        "isomorphic-fetch": "^2.2.1",
        "modular-scale": "^4.4.1",
        "moment": "^2.20.1",
        "next": "^4.2.1",
        "next-apollo": "^1.0.4",
        "path": "^0.12.7",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-apollo": "^2.0.4",
        "react-dom": "^16.2.0",
        "react-toggle-display": "^2.2.0",
        "styled-components": "^2.3.0"
    }
}
